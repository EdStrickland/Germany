/* detail */
ul {
	margin: 0;
	padding: 0;
}

li {
	list-style: none;
}

.school-info-box {
	display: flex;
	width: 100%;
	height: 240px;
	background-color: #F2F6F9;
	padding: 50px 100px 10px 100px;
}

.school-img-box {
	height: 150px;
	width: 150px;
}

.school-img-box img {
	height: 150px;
	width: 150px;
}

.school-info-details {
	height: 150px;
	width: calc(100% - 150px);

	padding-left: 20px;
}

.school-info-data ul {
	display: flex;
	margin: 0;
	padding: 0;
}

.school-info-data li {
	width: 150px;
	text-align: center;
	text-decoration: none;
	list-style: none;
}

.page-content-changer ul {
	display: flex;
	padding: 0 100px;

	background-color: #F2F6F9;

	border-bottom: 2px solid #E4E7ED;
}

.page-content-changer li {
	display: block;
	width: 80px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-weight: bold;
	font-size: 14px;
}

.page-content-changer .selected {
	color: #1c7fe6;
	border-bottom: 2px solid #1c7fe6;
}

.page-content-changer a:hover{
	cursor: pointer;
}

.page-content-content {
	padding: 0 100px;
}

.page-content-block {
	display: none;
}

.page-content-content .display{
	display: block;
}



table {
	display: block;
	
	width: 1060px;

    border-right: 0;
    border-bottom: 0;
	border-radius: 2px;
	
	overflow-y: hidden;
}

thead {
	display: block;
	
	width: 1060px;
	height: 50px;
	line-height: 50px;

	border-radius: 3px;
	background-color: #F0F2F5;
    overflow-x: hidden;
}

tbody {
    /*height: 651px;*/
    /*overflow: auto; */
    display: block;
    width: 100%;
}

tbody tr {
	border-bottom: 1px solid #F0F2F5;
	height: 50px;
	line-height: 45px;
}

tbody tr:hover {
	cursor: pointer;
}

td {
    padding:  5px 10px;
}

tr {
    display: block;
}

tr:hover {
    background-color: #efefef;
}

td, th {
    width: 151px;   
}

.table-ranking {
	display: block;
	height: 44px;	
	line-height: 44px;
	margin: 0 3px;
	background-color: #fff;
}

.table-container {
	width: 1060px;

	margin: 0 auto;
	padding-top: 60px;
}